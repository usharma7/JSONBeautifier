//metadata about the plugin
//contains description, version, name, etc

{

  "manifest_version": 2,
  "name": "JSONBeautifier",
  "description": "This extension allows user to easily delimit those pesky JSONs in Solr",
  "version":"0.5",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions":[
    "activeTab",
    "storage"
  ],

  "background": {
    "scripts" :["popup.js"],
    "persistent": true
  }

}
